<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <webscale_tools>
            <general>
                <enabled>0</enabled>
                <access_key backend_model="Magento\Config\Model\Config\Backend\Encrypted"></access_key>
            </general>
            <maintenance>
                <enabled>0</enabled>
            </maintenance>
            <caching>
                <invalidate_after>60</invalidate_after>
            </caching>
        </webscale_tools>
    </default>
</config>
